Is this call ambigous ? yes!

#include <iostream>

using namespace std;
class rohit{
    
    int x1;
public:
    rohit(){
      cout<<"hi rohit";
    }
    
    rohit(int x = 99){
        cout<<"hi rohit11";
    }
};
int main()
{
    rohit obj;
    return 0;
}

